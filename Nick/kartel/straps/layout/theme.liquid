    $(window).load(function(){
      setTimeout(function(){
        console.log('timer loaded');
        $(".trustblox-1-timer-wrap").wrap('<div class="trustBloxTimer"></div>');
        $('.trustBloxTimer').parent().nextAll('.header:first').addClass('TimerHeight');
        $(window).scroll(function(){
          $('.trustBloxTimer').parent().nextAll('.header:first').addClass('scrolling');
          $('.trustBloxTimer').parent().nextAll('#header:first').removeClass('scrollMobile');
          var scrollposition = $(window).scrollTop();
          if(scrollposition==0)
          {
            $('.trustBloxTimer').parent().nextAll('.header:first').removeClass('scrolling');
            $('.trustBloxTimer').parent().nextAll('#header:first').addClass('scrollMobile');
          }
        });
        var width = $(window).width();
        if(width<=764)
        {
          $('.trustBloxTimer').parent().nextAll('#header:first').addClass('scrollMobile');
        }
      }, 3000); 
    });
</script>
