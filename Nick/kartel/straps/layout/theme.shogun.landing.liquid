<script type="text/javascript">
  $(document).ready(function(){
    
    //////// To add wrapper of timer //////////
    $(".trustblox-1-timer-wrap").wrap('<div class="trustBloxTimer"></div>');
    
    $('div.content:eq(1)').css('padding',0);
    $('ul.slides > li').each(function(i){
     if($(this) .hasClass('HasvariantImage'))
        {
            console.log(i);
        	$('ul.flex-control-nav > li :eq('+i+')').parent().hide();
        }
    })
  });
  $('.select select').on('change',function(){
    var varObj = $('.select select').find(':selected');
     var p = varObj.data('price');
     p=p/100;
    p=p.toFixed(2);
    $('.money').attr('data-currency-gbp','£'+p);
    $('.money').text('£'+p);
     var cookieCurrency = Currency.cookie.read();
     var fp=Currency.convert(p,'GBP',cookieCurrency);
    
    
     fp=fp.toFixed(2);
     //alert(fp);
  
    if(cookieCurrency=='USD')
    {
       $('.money').attr('data-currency-usd','&dollar;'+fp);
       $('.current_price > span').html('&dollar;'+fp);
      
    }
    else if (cookieCurrency=='GBP')
    {
      $('.current_price > span').html('&pound;'+fp);
      //$('.money').attr('data-currency-gbp','&pound;'+fp);
    }
    
    else if (cookieCurrency=='EUR')
    {
       $('.money').attr('data-currency-eur','&euro;'+fp);
      $('.current_price > span').html('&euro;'+fp);
      
    }
    var vTitle = varObj.text().trim();
    console.log("*(*(*"+vTitle);
    
    var tmp = false;
    
    $('ul.slides > li').each(function(i){
      
      console.log("**** "+$(this).attr('data-variant'));
      
      if($(this).attr('data-variant') === vTitle)
      {
         tmp = true;
        
        $('ul.flex-control-nav > li :eq('+i+')').trigger('click');
      }
      
      //$('ul.flex-control-nav > li :eq(2)').trigger('click')
    });
    
    if(!tmp)
    {
       $('ul.flex-control-nav > li :eq(0)').trigger('click');
    }

  });        
  </script>
