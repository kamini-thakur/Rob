      </div>

      <div class="li">
        {% if settings.menu-link10 and settings.menu-link10 != '' %}
        {% capture menu %}{{ settings.megamenu2_text | handleize }}{% endcapture %}
        <a href="{{ settings.megamenu10_link }}">{{ menu | replace:"-"," "}} {% if linklists[menu].links.size > 0 %}<span class="icon-down"></span>{% endif %}</a>
        {% if settings.megamenu10_sub_toggle and settings.megamenu10_sub_toggle != '' %}
        <div class="ul submenu big-submenu">
          <div class="container">
            <div class="row">
              {% for i in (1..4) %}     
              {% capture collection_iteration %}megamenu10_sub_col__{{ i }}{% endcapture %}
              {% capture collection_name %}{{ settings[collection_iteration] }}{% endcapture %}   
              <div class="col-md-2 section">
                <span class="title">{{ linklists[collection_name].title  }}</span>
                {% for link_sub in linklists[collection_name].links  %}
                <span class="smrow"><a href="{{ link_sub.url }}">{{ link_sub.title }}
                   {% assign link_name = link_sub.title | escape | downcase %}
                    {% assign new_label_10_1  = settings.megamenu10_new_1_label  | replace: ' ,', ',' | replace: ', ', ',' | split: ',' %} 
                    {% for link_label in new_label_10_1 %} 
                    {% if link_label  != "" and link_label ==  link_name %}
                    <span class="new" >new</span>
                    {% endif %}
                    {% endfor %}

                    {% assign sell_label_10_1  = settings.megamenu10_sale_1_label  | replace: ' ,', ',' | replace: ', ', ',' | split: ',' %} 
                    {% for link_label in sell_label_10_1 %} 
                    {% if link_label != "" and link_label ==  link_name %}
                    <span class="sale" >sale</span>
                    {% endif %}
                    {% endfor %}     
                  </a></span>
                {% endfor %}
              </div>
              {% endfor %}
              {% if settings.megamenu10_bg_toggle %}
              <div class="col-md-4 section">
                <div class="img">
                  <img src="{{ 'megamenu10_bg.png' | asset_url }}" alt="" />
                  <div class="img-title">
                    <span class="main">Leather Jacket</span>
                    <span class="sub">Men's fashion</span>
                  </div>
                </div>
              </div>
              {% endif %}
            </div>
          </div>
        </div>
        {% else %}
        <div class="ul {% if linklists[menu] and linklists[menu].links.size > 0 %} submenu small{% endif %}">
          {% if linklists[menu].links.size > 0 %}<span class="title">{{ menu }}</span>{% endif %}
          {% for link in linklists[menu].links %}
          <span class="smrow"><a href="{{ link.url }}">{{ link.title }}</a></span>
          {% endfor %}
        </div>
        {% endif %}
        {% endif %}
      </div>
    </div>
  </nav>
</div>


