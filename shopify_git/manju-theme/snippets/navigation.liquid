        {% if settings.menu-link1 and settings.menu-link1 != '' %}
        {% capture menu %}{{ settings.megamenu1_text | handleize }}{% endcapture %}
        <a href="{{ settings.megamenu1_link }}">{{ menu | replace:"-"," "}} {% if linklists[menu].links.size > 0 %}<span class="icon-down"></span>{% endif %}</a>
        {% if settings.megamenu1_sub_toggle and settings.megamenu1_sub_toggle != '' %}
        <div class="ul submenu big-submenu">
          <div class="container">
            <div class="row">
              {% for i in (1..4) %}                
              {% capture collection_iteration %}megamenu2_sub_col__{{ i }}{% endcapture %}
              {% capture collection_name %}{{ settings[collection_iteration] }}{% endcapture %}  
              {% if  i == 1  %}
              <span class="title col-lg-12">{{ linklists[collection_name].title  }}</span>
              {% endif %}
              {% for link_sub in linklists[collection_name].links  %}
              {% assign modulovalue = forloop.index | modulo:5 %}  
              {% if modulovalue == 1 %}	
              <div class="col-lg-3">
                {% endif %}
                <span class="smrow"><a href="{{ link_sub.url }}">{{ link_sub.title }}
                  {% assign link_name = link_sub.title | escape | downcase %}
                    {% assign new_label_1_1  = settings.megamenu1_new_1_label  | replace: ' ,', ',' | replace: ', ', ',' | split: ',' %} 
                    {% for link_label in new_label_1_1 %} 
                    {% if link_label  != "" and link_label ==  link_name %}
                    <span class="new" >new</span>
                    {% endif %}
                    {% endfor %}

                    {% assign sell_label_1_1  = settings.megamenu1_sale_1_label  | replace: ' ,', ',' | replace: ', ', ',' | split: ',' %} 
                    {% for link_label in sell_label_1_1 %} 
                    {% if link_label != "" and link_label ==  link_name %}
                    <span class="sale" >sale</span>
                    {% endif %}
                    {% endfor %}
                  
                  
                  </a></span>
                {% if modulovalue == 0  or forloop.last %}
              </div>
              {% endif %}
              {% endfor %}
              {% endfor %}
              {% if settings.megamenu1_bg_toggle %}
              <div class="col-md-4 section">
                <div class="img">
                  <img src="{{ 'megamenu1_bg.png' | asset_url }}" alt="" />
                  <div class="img-title">
                    <span class="main">Leather Jacket</span>
                    <span class="sub">Men's fashion</span>
                  </div>
                </div>
              </div>
              {% endif %}
            </div>
          </div>
        </div>
        {% else %}
        <div class="ul {% if linklists[menu] and linklists[menu].links.size > 0 %} submenu small{% endif %}">
          {% if linklists[menu].links.size > 0 %}<span class="title">{{ menu }}</span>{% endif %}
          {% for link in linklists[menu].links %}
          <span class="smrow"><a href="{{ link.url }}">{{ link.title }}</a></span>
          {% endfor %}
